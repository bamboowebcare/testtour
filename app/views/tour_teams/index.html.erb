<div class="row">
     <div class="large-3 columns">
        <%= render "layouts/googlead" %>
 </div> 
<!-- Main Content Section -->
<!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
<div class="large-9 columns">

<h1>Jouw Tour Teams</h1>

<table>
  <thead>
    <tr>
      <th>Tour Team Naam</th>
      <th>User</th>
      <th>Poule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tour_teams.each do |tour_team| %>
      <tr>
        <td><%= tour_team.name %></td>
        <td><%= tour_team.user.name %></td>
        <td><%= tour_team.poule.name %></td>
        <td><%= link_to 'Show', tour_team %></td>
        <td><%= link_to 'Edit', edit_tour_team_path(tour_team) %></td>
        <td><%= link_to 'Destroy', tour_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tour team', new_tour_team_path %>
</div>